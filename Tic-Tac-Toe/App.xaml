<Application x:Class="Tic_Tac_Toe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Tic_Tac_Toe"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor" Color="#f48a2e"/>
        <SolidColorBrush x:Key="ButtonColour" Color="#ffb966"/>
        <SolidColorBrush x:Key="LineColour" Color="#FF36E23C"/>

        <SolidColorBrush x:Key="OColour" Color="#0F0F0F"/>
        <SolidColorBrush x:Key="XColour" Color="#FDFFF5"/>
    </Application.Resources>
</Application>
