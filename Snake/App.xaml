<Application x:Class="Snake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColour">#1A062E</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColour">#191240</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColour">#533B75</SolidColorBrush>
        <SolidColorBrush x:Key="TextColour">#EBEBEB</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColour">#7F000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Droid Sans Mono</FontFamily>
    </Application.Resources>
</Application>
